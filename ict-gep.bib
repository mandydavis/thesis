@article{basowYouOweMe2011,
  title = {“{{You Owe Me}}”: {{Effects}} of {{Date Cost}}, {{Who Pays}}, {{Participant Gender}}, and {{Rape Myth Beliefs}} on {{Perceptions}} of {{Rape}}},
  author = {Basow, Susan A. and Minieri, Alexandra},
  date = {2011},
  journaltitle = {Journal of Interpersonal Violence},
  volume = {26},
  pages = {479--497},
  doi = {10.1177/0886260510363421},
  abstract = {Sexual social exchange theory was applied to perceptions of a date rape by manipulating the cost of the date and who paid in vignettes presented to 188 U.S. college students, who then rated the characters’ sexual expectations, blame, responsibility, and rape justifiability. Findings from this between-participant design partially supported predictions: When the man paid for an expensive date, men agreed more than did women that both characters should have expected sexual intercourse. Conversely, when the costs of an inexpensive date were split, the perpetrator was assigned the most blame, and women agreed more than men that no sexual expectations were warranted. Participant gender affected some responses, although level of rape myth acceptance was the main predictor of rape perceptions.},
  keywords = {rape perceptions;rape myth acceptance;sexual economics;social exchange theory;acquaintance rape},
  number = {3}
}

