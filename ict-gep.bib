@article{lanerEgalitarianDatersTraditionalist1998,
  title = {Egalitarian {{Daters}}/{{Traditionalist Dates}}},
  author = {Laner, Mary Riege and Ventrone, Nicole A.},
  date = {1998},
  journaltitle = {Journal of Family Issues},
  volume = {19},
  pages = {468--477},
  doi = {10.1177/019251398019004005},
  abstract = {This study replicates and expands Rose and Frieze's (1989) research, which found that young adults' scripts for dating maintain the traditional dominant/subordinate relationship between the sexes. In a 1993 study, Rose and Frieze looked at actual, rather than hypothetical, dates and reported essentially the same finding. The authors used Rose and Frieze's 1989 design with a larger sample and found that although daters may claim to be egalitarian, their dates follow the traditionalist pattern that Rose and Frieze described. In a second study, the authors asked for descriptions of first dates with persons whom respondents already knew from work, school, and so forth, rather than with the relative stranger about whom earlier versions had asked. Varying the level of familiarity with a partner did not produce a different result. Implications are drawn from the findings and suggestions for future research are offered.},
  number = {4}
}

