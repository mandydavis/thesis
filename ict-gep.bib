@article{ramirezjrWhenOnlineMeets2007,
  title = {When {{Online Meets Offline}}: {{The Effect}} of {{Modality Switching}} on {{Relational Communication}}},
  author = {Ramirez Jr, Artemio and Zhang, Shuangyue},
  date = {2007-09-01},
  journaltitle = {Communication Monographs},
  volume = {74},
  pages = {287--310},
  issn = {0363-7751},
  doi = {10.1080/03637750701543493},
  abstract = {Collaborative partnerships developed via text-based computer-mediated communication (CMC) commonly shift interactions to alternative formats. Extant research indicates that shifting from one modality to another, or ?modality switching,? can have profound positive and negative effects on relational outcomes. Drawing on social presence theory (Short, Williams, Walther, 1992, 1996), the present study examined the influence of meeting FtF after varying lengths of time interacting via CMC on relational communication. Consistent with predictions, remaining online yielded greater intimacy and social attraction than the other conditions in which FtF contact occurred. With respect to the CMC conditions, modality switching modestly enhanced relational outcomes in the ?early? switching partnerships but more strongly dampened those of ?late? switching ones.},
  number = {3}
}

